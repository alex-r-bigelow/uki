{
  "name": "uki",
  "version": "0.1.0",
  "description": "Minimal, d3-based Model-View library",
  "main": "build/uki.umd.min.js",
  "module": "build/uki.es.js",
  "esnext": "build/uki.es.js",
  "jsnext:main": "build/uki.es.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build_umd": "./node_modules/.bin/rollup -c rollup.umd.config.js && ./node_modules/.bin/uglifyjs ./build/uki.umd.js -c -m -o ./build/uki.umd.min.js && cp ./build/uki.umd.min.js ./examples/umd/uki.umd.min.js",
    "build_es": "./node_modules/.bin/rollup -c rollup.es.config.js",
    "build": "npm run build_umd; npm run build_es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-r-bigelow/uki.git"
  },
  "author": "Alex Bigelow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alex-r-bigelow/uki/issues"
  },
  "homepage": "https://github.com/alex-r-bigelow/uki#readme",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "uglify-js": "^3.0.20"
  }
}
